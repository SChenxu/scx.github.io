<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

    <!--设置别名，在mapper.xml文件中，返回值不用在写包名了，直接写类名即可-->
    <typeAliases>
        <package name="com.songchenxu.domain"/>
    </typeAliases>
    <!-- sql映射文件的位置 -->
    <mappers>
        <package name="com.songchenxu.dao"/>
    </mappers>

</configuration>
